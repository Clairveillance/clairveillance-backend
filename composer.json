{
  "name": "clairveillance/clairveillance",
  "type": "project",
  "version": "0.1.0",
  "license": "MIT",
  "authors": [
    {
      "name": "Clairveillance",
      "email": "info@clairveillance.ch",
      "homepage": "https://clairveillance.ch"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "barryvdh/laravel-debugbar": "^3.6",
    "darkaonline/l5-swagger": "^8.0",
    "friendsofphp/php-cs-fixer": "^3.3",
    "fruitcake/laravel-cors": "^2.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "laravel/framework": "^8.65|^8.0",
    "laravel/sanctum": "^2.11",
    "laravel/tinker": "^2.5",
    "nuwave/lighthouse": "^5.31",
    "predis/predis": "^1.1",
    "spatie/laravel-query-builder": "^4.0"
  },
  "require-dev": {
    "eduarguz/shift-php-cs": "^3.0",
    "facade/ignition": "^2.5",
    "fakerphp/faker": "^1.9.1",
    "laravel/framework": "^8.65|^8.0",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.4.4|^1.4.2",
    "nunomaduro/collision": "^5.10|^5.0",
    "overtrue/phplint": "^3.0",
    "phpunit/phpunit": "^9.5.10|^9.3.3"
  },
  "autoload": {
    "psr-4": {
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/",
      "App\\": "src/App/",
      "Domain\\": "src/Domain/",
      "Infrastructure\\": "src/Infrastructure"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
